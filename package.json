{
  "name": "airmessage-react",
  "version": "1.1.0",
  "author": "Tagavari",
  "description": "iMessage for the web, Windows, and Linux",
  "private": true,
  "scripts": {
    "start": "cross-env SNOWPACK_PUBLIC_VERSION=$npm_package_version SNOWPACK_PUBLIC_URL_PREFIX=/ snowpack dev",
    "build": "eslint src browser electron-main electron-renderer --ext .js,jsx,.ts,.tsx && cross-env SNOWPACK_PUBLIC_VERSION=$npm_package_version SNOWPACK_PUBLIC_RELEASE_HASH=$(git rev-parse --short HEAD) SNOWPACK_PUBLIC_BUILD_DATE=$(date +%s) SNOWPACK_PUBLIC_ELECTRON=false SNOWPACK_PUBLIC_URL_PREFIX=/ snowpack build && workbox generateSW workbox-config.js",
    "electron-launch": "cross-env ELECTRON_START_URL=http://localhost:8080 electron electron-main/electron.js",
    "electron-start": "cross-env SNOWPACK_PUBLIC_ELECTRON=true SNOWPACK_PUBLIC_URL_PREFIX= concurrently \"snowpack dev --config snowpack-electron.config.js --open none\" \"wait-on tcp:8080 && npm run electron-launch\"",
    "electron-build": "cross-env SNOWPACK_PUBLIC_ELECTRON=true SNOWPACK_PUBLIC_URL_PREFIX= snowpack build --config snowpack-electron.config.js",
    "electron-package": "npm run electron-build && electron-builder --dir",
    "electron-distribute": "npm run electron-build && electron-builder",
    "test": "echo \"This template does not include a test runner by default.\" && exit 1",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@snowpack/plugin-react-refresh": "^2.4.0",
    "@snowpack/plugin-run-script": "^2.3.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/plugin-webpack": "^2.3.0",
    "@types/bytebuffer": "^5.0.42",
    "@types/gapi": "^0.0.39",
    "@types/gapi.auth2": "^0.0.53",
    "@types/gapi.people": "^1.0.4",
    "@types/luxon": "^1.25.1",
    "@types/markdown-to-jsx": "^6.11.3",
    "@types/pako": "^1.0.1",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/spark-md5": "^3.0.2",
    "@types/ua-parser-js": "^0.7.35",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "electron": "^12.0.0",
    "electron-builder": "^22.9.1",
    "eslint": "^7.20.0",
    "eslint-plugin-react": "^7.22.0",
    "snowpack": "^3.0.1",
    "typescript": "^4.1.5",
    "wait-on": "^5.2.1",
    "workbox-cli": "^6.1.0"
  },
  "dependencies": {
    "@electron/typescript-definitions": "^8.8.0",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@sentry/react": "^6.1.0",
    "bytebuffer": "^5.0.1",
    "electron-context-menu": "^2.4.0",
    "firebase": "^8.2.6",
    "libphonenumber-js": "^1.9.10",
    "luxon": "^1.25.0",
    "markdown-to-jsx": "^7.1.1",
    "pako": "^2.0.3",
    "react": "^17.0.1",
    "react-anchorme": "^2.1.1",
    "react-dom": "^17.0.1",
    "react-flip-toolkit": "^7.0.13",
    "spark-md5": "^3.0.1",
    "ua-parser-js": "^0.7.23",
    "uuid": "^8.3.2"
  }
}
