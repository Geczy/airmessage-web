{
  "name": "airmessage-react",
  "version": "1.4.3",
  "author": "Tagavari",
  "description": "iMessage for the web",
  "private": true,
  "sideEffects": [
    "**/*.css",
    "./{browser,windows/web}/init.ts"
  ],
  "scripts": {
    "dev:p": "pnpm dev -p 3001",
    "dev:ssl": "local-ssl-proxy --source 443 --target 3001 --cert ./web.airmessage.org.pem --key ./web.airmessage.org-key.pem",
    "dev:local": "pnpm dev -p 3001 | local-ssl-proxy --source 443 --target 3001 --cert ./localhost.pem --key ./localhost-key.pem",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@jest/types": "^28.1.3",
    "@testing-library/react": "^13.3.0",
    "@types/bytebuffer": "^5.0.43",
    "@types/gapi": "^0.0.42",
    "@types/gapi.auth2": "^0.0.56",
    "@types/gapi.people": "^1.0.9",
    "@types/jest": "^28.1.5",
    "@types/luxon": "^2.3.2",
    "@types/node": "^18.0.4",
    "@types/pako": "^2.0.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-transition-group": "^4.4.5",
    "@types/spark-md5": "^3.0.2",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "classnames": "^2.3.1",
    "eslint": "^8.19.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "raw-loader": "^4.0.2",
    "sass": "^1.53.0",
    "ts-jest": "^28.0.6",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/base": "5.0.0-alpha.89",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.0",
    "@mui/system": "^5.9.0",
    "@mui/types": "^7.1.4",
    "@mui/utils": "^5.9.0",
    "@sentry/react": "^7.6.0",
    "bytebuffer": "^5.0.1",
    "firebase": "^9.9.0",
    "js-base64": "^3.7.2",
    "libphonenumber-js": "^1.10.8",
    "linkify-react": "^3.0.4",
    "linkifyjs": "^3.0.5",
    "luxon": "^3.0.1",
    "markdown-to-jsx": "^7.1.7",
    "next": "12.2.2",
    "pako": "^2.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-transition-group": "^4.4.2",
    "spark-md5": "^3.0.2",
    "ua-parser-js": "^1.0.2",
    "uuid": "^8.3.2"
  }
}
